model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  deletedAt DateTime?
}

model Transaction {
  id        Int      @id @default(autoincrement())
  userId    Int
  type      String
  amount    Float
  createdAt DateTime @default(now())
}

model SavingGoal {
  id        Int      @id @default(autoincrement())
  userId    Int
  goal      String
  createdAt DateTime @default(now())
}
